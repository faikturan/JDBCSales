<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Properties>
  <![CDATA[]]>
    <Property name="PATTERN"><![CDATA['%d{yyyy-MM-dd HH:mm:ss.SSS} %t [%-5level] %logger{36}.%M - Line.%L - %msg%n']]></Property>
  </Properties>
  <Appenders>
  <Console name="STDOUT" target="SYSTEM_OUT">
  <PatternLayout pattern="${PATTERN}"/>
  </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>